<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Validitate</servlet-name>
        <servlet-class>Validitate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ppost</servlet-name>
        <servlet-class>Ppost.Ppost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pposte</servlet-name>
        <servlet-class>Pposte</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Profile</servlet-name>
        <servlet-class>Profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet1</servlet-name>
        <servlet-class>NewServlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Validitate</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ppost</servlet-name>
        <url-pattern>/Ppost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pposte</servlet-name>
        <url-pattern>/Pp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Profile</servlet-name>
        <url-pattern>/Profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet1</servlet-name>
        <url-pattern>/NewServlet1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
